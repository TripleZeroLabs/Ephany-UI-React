# .env.example
# Example environment configuration for Ephany UI

# Where /api should proxy to in development.
# Developers should update this to match their own Ephany Framework backend.
VITE_PROXY_TARGET=http://localhost:8000
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_KEY=YOUR-EPHANY-API-KEY

# Use mock data instead of hitting a real backend API.
# Set to 'true' for offline development.
VITE_USE_MOCK_DATA=false
